{
  "compilerOptions": {
    "outDir": "./dist",
      "allowJs": true,
      "target": "es5",
      "allowUnreachableCode": false,
      "noImplicitReturns": true,
      "noImplicitAny": false,
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "lib": ["es2016", "dom"],
    "sourceMap": true,
    "incremental": true,
    "skipLibCheck": true,
    "types": [
      "node"
    ],
  },
  "exclude": [
    "node_modules"
  ]
}
